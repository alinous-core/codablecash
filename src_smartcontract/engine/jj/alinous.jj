UNICODE_INPUT = true;
OUTPUT_LANGUAGE = "c++";
NAMESPACE = "alinouslang";
OUTPUT_DIRECTORY = "src_gen/alinous_lang/";
FORCE_LA_CHECK = false;
ERROR_REPORTING = true;
PARSER_INCLUDES = "engine/sc/alinouslang.h";
//STATIC = false; // ignored by c++
DEBUG_PARSER=false;
}

PARSER_BEGIN(AlinousLang)

