void sqlFunctionCallArguments(SQLFunctionCall* exp) : {
	AbstractSQLExpression* ex = nullptr;
	Token* t = nullptr;
}
{
	[
		ex=sqlDistinctArgument() {
			exp->addArgument(ex);
			exp->setPosition(ex);
		}
		
		(
			t="," {
				exp->setPosition(t);
			}
			ex=sqlExpression() {
				exp->addArgument(ex);
				exp->setPosition(ex);
			}
			
		)*
	]
}

AbstractSQLExpression* sqlDistinctArgument() : {
	SQLDistinctArgument* dist = nullptr;
	AbstractSQLExpression* ex = nullptr;
	Token* t = nullptr;
}
{
	[LOOKAHEAD(2)
		t=<DISTINCT> {
			dist = new SQLDistinctArgument();
			dist->setPosition(t);
		}
	]
	ex=sqlExpression() {
		if(t != nullptr){
			dist->setPosition(ex);
			dist->setExpression(ex);
			ex = dist;
		}
	}
	{
		return ex;
	}
	
}

